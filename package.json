{
	"author": "Rafa≈Ç Majewski",
	"bugs": {
		"url": "https://github.com/Rafal-Majewski/nodejs-app-template/issues"
	},
	"dependencies": {
		"dotenv": "16.3.1",
		"envalid": "8.0.0"
	},
	"description": "Node.js app template",
	"devDependencies": {
		"@stylistic/eslint-plugin": "1.7.2",
		"@types/node": "20.11.28",
		"@typescript-eslint/eslint-plugin": "7.0.0",
		"@typescript-eslint/parser": "6.17.0",
		"@vitest/coverage-v8": "1.2.0",
		"eslint": "8.57.0",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-sort-destructure-keys": "1.5.0",
		"nodemon": "3.0.2",
		"prettier": "3.2.5",
		"release-it": "17.0.1",
		"testcontainers": "10.7.2",
		"tsx": "4.7.0",
		"typescript": "5.3.3",
		"vitest": "1.5.0"
	},
	"homepage": "https://github.com/Rafal-Majewski/nodejs-app-template#readme",
	"license": "CC0-1.0",
	"main": "./src/main.ts",
	"name": "nodejs-app-template",
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rafal-Majewski/nodejs-app-template.git"
	},
	"scripts": {
		"compile": "tsc",
		"compile:check": "tsc --noEmit",
		"dev": "tsx ./src/main.ts",
		"dev:watch": "nodemon --watch ./src --exec tsx ./src/main.ts",
		"eslint:check": "eslint --max-warnings 0 .",
		"eslint:fix": "eslint --max-warnings 0 --fix .",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"release": "release-it",
		"vitest:check": "vitest run --passWithNoTests --coverage",
		"vitest:check:watch": "vitest --watch --passWithNoTests --coverage"
	},
	"type": "module",
	"version": "1.7.1"
}
