{
	"author": "Rafa≈Ç Majewski",
	"bugs": {
		"url": "https://github.com/Rafal-Majewski/nodejs-app-template/issues"
	},
	"dependencies": {
		"dotenv": "16.3.1",
		"envalid": "8.0.0"
	},
	"description": "SvelteKit app template",
	"devDependencies": {
		"@types/node": "20.10.0",
		"@typescript-eslint/eslint-plugin": "6.12.0",
		"@typescript-eslint/parser": "6.12.0",
		"@vitest/coverage-v8": "0.34.6",
		"eslint": "8.54.0",
		"eslint-plugin-import": "2.29.0",
		"nodemon": "3.0.1",
		"prettier": "3.1.0",
		"release-it": "16.2.1",
		"testcontainers": "10.3.1",
		"tsx": "4.6.1",
		"typescript": "5.3.2",
		"vitest": "0.34.6"
	},
	"homepage": "https://github.com/Rafal-Majewski/nodejs-app-template#readme",
	"license": "CC0-1.0",
	"main": "./dist/src/main.js",
	"name": "nodejs-app-template",
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rafal-Majewski/nodejs-app-template.git"
	},
	"scripts": {
		"compile": "tsc",
		"compile:check": "tsc --noEmit",
		"dev": "tsx ./src/main.ts",
		"dev:watch": "nodemon --watch ./src --exec tsx ./src/main.ts",
		"eslint:check": "eslint --max-warnings 0 .",
		"eslint:fix": "eslint --fix .",
		"formatting:check": "prettier --check .",
		"formatting:fix": "prettier --write .",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"release": "release-it",
		"test": "vitest --run --passWithNoTests --coverage",
		"typescript:check": "tsc --noEmit",
		"vitest:check": "vitest run --passWithNoTests --coverage",
		"vitest:check:watch": "vitest --watch --passWithNoTests --coverage"
	},
	"type": "module",
	"version": "1.7.1"
}
